{
  "Logging": {
    "LogLevel": {
      "Microsoft.AspNetCore.Diagnostics.ExceptionHandlerMiddleware": "None",
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "RabbitMQ": {
    "Host": "YourHost",
    "Username": "YourName",
    "Password": "YourPass",
    "Port": 5672,
    "VirtualHost": "Your Virtual Host"
  },
  "ConnectionStrings": {
    "ConnectionStr": "Host=localhost;Port=YourPort;Database=URL_SHORTENER_DOMAIN;Username=YourName;Password=YourPass"
  },
  "Serilog": {
    "Using": [
      "Serilog.Sinks.Seq"
    ],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "Microsoft.AspNetCore.Diagnostics.ExceptionHandlerMiddleware": "Fatal",
        "Microsoft.Hosting.Lifetime": "Information",
        "System": "Warning"
      }
    },
    "WriteTo": [
      {
        "Name": "Seq",
        "Args": {
          "serverUrl": "Your Server Url",
          "apiKey": "YourApiKey",
          "retainedFileCountLimit": 10
        }
      }
    ],
    "Enrich": [
      "FromLogContext",
      "WithMachineName",
      "WithThreadId",
      "WithCorrelationId"
    ],
    "Properties": {
      "Application": "DomainService",
      "ServiceName": "DomainService",
      "Environment": "Development"
    }
  },
  "AllowedHosts": "*"
}


